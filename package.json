{
  "name": "opentrials",
  "version": "0.0.1",
  "description": "OpenTrials.net website",
  "engines": {
    "node": "6.9.5"
  },
  "main": "server.js",
  "scripts": {
    "pretest": "gulp build",
    "test": "node ./node_modules/.bin/istanbul cover _mocha -- --grep e2e --invert --require test/common",
    "posttest": "npm run lint",
    "pree2e": "gulp build",
    "e2e": "mocha --grep e2e --require test/e2e/common test/e2e",
    "lint": "eslint .",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "dev": "gulp dev",
    "debug": "node --inspect ./node_modules/.bin/gulp dev",
    "prestart": "gulp build",
    "start": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opentrials/opentrials.git"
  },
  "keywords": [
    "opentrials"
  ],
  "author": "Open Knowledge <info@okfn.org> (http://okfn.org)",
  "contributors": [
    "Vitor Baptista <vitor@vitorbaptista.com> (http://vitorbaptista.com)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opentrials/opentrials/issues"
  },
  "homepage": "https://github.com/opentrials/opentrials#readme",
  "devDependencies": {
    "coveralls": "^2.11.12",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.15.0",
    "factory-girl": "^3.1.0",
    "factory-girl-bookshelf": "^1.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "node-uuid": "^1.4.7",
    "selenium-webdriver": "^2.53.3",
    "should": "^11.1.0",
    "sinon": "^1.17.5"
  },
  "dependencies": {
    "bell": "^8.2.0",
    "bluebird": "^3.4.6",
    "blueimp-file-upload": "^9.12.1",
    "bookshelf": "^0.10.1",
    "boom": "^4.0.0",
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.7.4",
    "del": "^2.2.2",
    "dotenv": "^2.0.0",
    "emailjs": "^1.0.8",
    "glue": "^4.0.0",
    "good": "^7.0.2",
    "good-console": "^6.1.2",
    "gravatar": "^1.5.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-imagemin": "^3.0.3",
    "gulp-load-plugins": "^1.3.0",
    "gulp-nodemon": "^2.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev-all": "^0.8.24",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "hapi": "^15.0.3",
    "hapi-auth-cookie": "^6.1.1",
    "hapi-context-credentials": "^2.0.0",
    "hapi-raven": "^4.3.1",
    "inert": "^4.0.2",
    "joi": "^9.0.4",
    "jquery-ui": "^1.12.0",
    "jquery.mmenu": "^5.6.3",
    "knex": "^0.12.0",
    "lodash": "^4.15.0",
    "marked": "^0.3.6",
    "moment": "^2.15.0",
